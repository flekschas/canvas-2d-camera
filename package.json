{
  "name": "canvas-camera-2d",
  "version": "0.1.0",
  "description": "A canvas wrapper for a 2D camera",
  "author": "Fritz Lekschas",
  "keywords": [
    "canvas",
    "2d",
    "camera",
    "webgl"
  ],
  "license": "MIT",
  "main": "dist/canvas-camera-2d.js",
  "module": "src/index.js",
  "files": [
    "src/index.js",
    "dist/canvas-camera-2d.js",
    "dist/canvas-camera-2d.min.js"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint src/index.js rollup.config.js",
    "prepublishOnly": "npm run lint",
    "prerelease": "rm -rf dist/*; npm run build; zip -r dist.zip dist",
    "pretest": "npm run lint",
    "test": "node tests/index.js | tap-spec",
    "watch": "rollup -cw"
  },
  "dependencies": {
    "camera-2d": "flekschas/camera-2d",
    "gl-matrix": "^2.8.1",
    "key-pressed": "flekschas/key-pressed",
    "mouse-position": "^2.1.0",
    "mouse-pressed": "^1.0.0",
    "scroll-speed": "flekschas/scroll-speed"
  },
  "peerDependencies": {
    "camera-2d": "flekschas/camera-2d",
    "key-pressed": "flekschas/key-pressed",
    "mouse-position": "^2.1.0",
    "mouse-pressed": "^1.0.0",
    "scroll-speed": "flekschas/scroll-speed"
  },
  "devDependencies": {
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "rollup": "^0.64.1",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-terser": "^1.0.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1"
  },
  "homepage": "https://github.com/flekschas/canvas-2d-camera",
  "repository": {
    "type": "git",
    "url": "git://github.com/flekschas/canvas-2d-camera.git"
  },
  "bugs": {
    "url": "https://github.com/flekschas/canvas-2d-camera/issues"
  }
}
