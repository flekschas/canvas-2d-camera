{
  "name": "canvas-camera-2d",
  "version": "0.5.3",
  "description": "A canvas wrapper for a 2D camera",
  "author": "Fritz Lekschas",
  "keywords": [
    "canvas",
    "2d",
    "camera",
    "webgl"
  ],
  "license": "MIT",
  "main": "dist/canvas-camera-2d.js",
  "module": "src/index.js",
  "unpkg": "dist/canvas-camera-2d.min.js",
  "jsdelivr": "dist/canvas-camera-2d.min.js",
  "files": [
    "src/index.js",
    "dist/canvas-camera-2d.js",
    "dist/canvas-camera-2d.min.js"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint src rollup.config.js",
    "precommit": "pretty-quick --staged",
    "prepublishOnly": "npm run lint",
    "prerelease": "rm -rf dist/*; npm run build; zip -r dist.zip dist",
    "pretest": "npm run lint",
    "watch": "rollup -cw"
  },
  "dependencies": {
    "camera-2d-simple": "^2.0.0-rc1",
    "gl-matrix": "^2.8.1",
    "key-pressed": "flekschas/key-pressed",
    "mouse-position": "^2.1.0",
    "mouse-pressed": "^1.0.0",
    "scroll-speed": "flekschas/scroll-speed"
  },
  "peerDependencies": {
    "camera-2d-simple": "^2.0.0-rc1",
    "key-pressed": "flekschas/key-pressed",
    "mouse-position": "^2.1.0",
    "mouse-pressed": "^1.0.0",
    "scroll-speed": "flekschas/scroll-speed"
  },
  "devDependencies": {
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^v3.1.0",
    "husky": "^1.1.2",
    "prettier": "1.14.3",
    "pretty-quick": "^1.8.0",
    "rollup": "^0.66.6",
    "rollup-plugin-buble": "^0.19.4",
    "rollup-plugin-terser": "^3.0.0"
  },
  "homepage": "https://github.com/flekschas/canvas-camera-2d",
  "repository": {
    "type": "git",
    "url": "git://github.com/flekschas/canvas-camera-2d.git"
  },
  "bugs": {
    "url": "https://github.com/flekschas/canvas-camera-2d/issues"
  }
}
