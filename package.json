{
  "name": "@flekschas/dom-2d-camera",
  "version": "0.5.5",
  "description": "A wrapper for attaching a 2D camera to a DOM element",
  "author": "Fritz Lekschas",
  "keywords": [
    "dom",
    "canvas",
    "2d",
    "camera",
    "webgl"
  ],
  "license": "MIT",
  "main": "dist/dom-2d-camera.js",
  "module": "dist/dom-2d-camera.esm.js",
  "unpkg": "dist/dom-2d-camera.min.js",
  "jsdelivr": "dist/dom-2d-camera.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint src rollup.config.js",
    "precommit": "pretty-quick --staged",
    "prepublishOnly": "npm run lint",
    "prerelease": "rm -rf dist/*; npm run build; zip -r dist.zip dist",
    "pretest": "npm run lint",
    "watch": "rollup -cw"
  },
  "dependencies": {
    "camera-2d-simple": "~2.0.0-rc2",
    "gl-matrix": "~3.1.0",
    "is-key-down": "~0.1.0",
    "mouse-position": "~2.1.0",
    "mouse-pressed": "~1.0.0",
    "scroll-speed": "flekschas/scroll-speed"
  },
  "peerDependencies": {
    "gl-matrix": "~3.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^v6.10.0",
    "husky": "^4.2.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "rollup": "^1.31.0",
    "rollup-plugin-terser": "^5.2.0"
  },
  "homepage": "https://github.com/flekschas/dom-2d-camera",
  "repository": {
    "type": "git",
    "url": "git://github.com/flekschas/dom-2d-camera.git"
  },
  "bugs": {
    "url": "https://github.com/flekschas/dom-2d-camera/issues"
  }
}
